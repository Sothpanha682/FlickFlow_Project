{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/components/:path*",
      "destination": "/components/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "/static/:path*"
    }
  ]
}
